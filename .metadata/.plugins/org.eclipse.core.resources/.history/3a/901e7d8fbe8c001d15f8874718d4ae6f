package outputs;

public class Exam3 {
	public static void main(String[] args) {
		
		//숫자형 뒤로갈수록 더 높은 숫자 할당 가능
		//정수
		byte a = 1 ;
		short b = 111;
		int c = 1111;
		long d = 123333L ;   
		System.out.printf("a:%d, b:%d, c:%d, d:%d", a,b,c,d);
		System.out.println("\n");
		
		//실수(기본형이   double이라서 float쓰려면 뒤에 F를 붙혀야함)
		
		float a1 = 1.22F;
		double a2 = 1.2222222;
		System.out.printf("a1: %f, a2: %f", a1,a2);
	}
}
