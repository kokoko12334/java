		System.out.println('dddd');
		
		
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("입력:");
		
		String inputData = sc.next();
		
		System.out.println("입력받은데이터:"+ inputData);
		
		
	}
	

	
